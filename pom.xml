    </dependency>
    <dependency>
      <groupId>org.qsardb.conversion</groupId>
      <artifactId>sdfile</artifactId>
      <version>1.1-SNAPSHOT</version>
      <type>jar</type>
    </dependency>
    <dependency>
      <groupId>org.qsardb.cargo</groupId>
      <artifactId>ucum</artifactId>
      <version>1.1-SNAPSHOT</version>
      <type>jar</type>
    </dependency>
    <dependency>
      <groupId>org.qsardb.resolution</groupId>
      <artifactId>doi</artifactId>
      <version>1.1-SNAPSHOT</version>
      <type>jar</type>
    </dependency>
    <dependency>
      <groupId>org.qsardb</groupId>
      <artifactId>validation</artifactId>
      <version>1.1-SNAPSHOT</version>
      <type>jar</type>
      <exclusions>
        <exclusion>
          <groupId>org.openscience.cdk</groupId>
          <artifactId>cdk-qsar</artifactId>
        </exclusion>
